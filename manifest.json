{
"manifest_version": 2,
    "name": "FiltERA",
    "description": "Filter threads by title",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://*.resetera.com/forums/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}
